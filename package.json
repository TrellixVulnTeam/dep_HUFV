{
  "name": "dep",
  "version": "0.6.1",
  "description": "A little Node.js dependency installer",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "pretest": "standard lib bin test/*js",
    "test": "tap \"test/*.js\"",
    "test-ci": "nyc tap 'test/*.js'",
    "posttest-ci": "npm run coverage",
    "precoverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage": "cat coverage.lcov | coveralls",
    "postcoverage": "rm coverage.lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/watilde/dep.git"
  },
  "bin": {
    "dep": "./bin/dep.js"
  },
  "keywords": [],
  "author": "Daijiro Wachi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/watilde/dep/issues"
  },
  "homepage": "https://github.com/watilde/dep#readme",
  "dependencies": {
    "cross-spawn": "^5.1.0",
    "fs-extra": "^4.0.0",
    "gunzip-maybe": "^1.4.1",
    "mkdirp": "^0.5.1",
    "node-gyp": "^3.6.2",
    "npm-package-arg": "^5.1.2",
    "npm-path": "^2.0.3",
    "request": "^2.81.0",
    "require-directory": "^2.1.1",
    "rimraf": "^2.6.1",
    "semver": "^5.4.1",
    "tar-fs": "^1.15.3",
    "tar-stream": "^1.5.4",
    "which": "^1.2.14",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "nyc": "^11.0.3",
    "standard": "^10.0.2",
    "standard-version": "^4.2.0",
    "strong-npm-ls": "^1.0.10",
    "tap": "^10.7.0"
  },
  "engine": {
    "node": ">=4.0.0"
  }
}
